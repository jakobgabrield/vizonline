func main(): none {


    /*
        simple if
    */
    if (3 < 4) {
        print("simple if statement"); 
        if (true) {
            print("first nested if/else -> true block");

            if (false) {
                print("second nested if/else -> true block");
            } else {
                print("second nested if/else -> false block");
            }


        } else {
            print("first nested if/else -> false block");
        }
    } 
}

